{
  "name": "goedr",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "start-frontend": "npm run build",
    "start-backend": "go run goedr.go",
    "build": "vite build",
    "dist": "concurrently \"GOOS=linux GOARCH=amd64 go build -o goedr-linux-amd64\" \"GOOS=darwin GOARCH=amd64 go build -o goedr-macos-amd64\" \"GOOS=darwin GOARCH=arm64 go build -o goedr-macos-arm\" \"GOOS=windows GOARCH=amd64 go build -o goedr-windows-amd64\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "animejs": "^4.1.2",
    "concurrently": "^9.2.1",
    "js-cookie": "^3.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.1"
  }
}
